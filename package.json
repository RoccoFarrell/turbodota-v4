{
  "name": "turbodota-v4",
  "version": "4.0.0",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "dev:msw-server": "concurrently \"npx ts-node-esm src/mocks/server.ts\" \"npm run dev\"",
    "build": "npx prisma generate && vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "test:e2e": "npx playwright test",
    "coverage": "npx vitest run --coverage",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "prisma": "npx prisma generate && npx prisma db push"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@skeletonlabs/skeleton": "2.5.1",
    "@skeletonlabs/tw-plugin": "0.2.4",
    "@sveltejs/adapter-vercel": "^4.0.3",
    "@sveltejs/kit": "^2.0.6",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "0.5.10",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/svelte": "^4.0.5",
    "@types/node": "^20.17.16",
    "@vitest/coverage-v8": "^1.1.1",
    "@vitest/ui": "^1.1.3",
    "autoprefixer": "10.4.16",
    "concurrently": "^8.2.2",
    "jsdom": "^23.2.0",
    "mdsvex": "^0.11.0",
    "node-fetch": "^3.3.2",
    "postcss": "8.4.32",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.0",
    "prisma": "^5.6.0",
    "supabase": "^1.115.4",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "svelte-confetti": "^1.3.1",
    "svelte-headless-table": "^0.17.7",
    "tailwindcss": "3.3.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.4.1",
    "tsx": "^4.7.0",
    "typescript": "^5.7.3",
    "vite": "^5.0.0",
    "vite-plugin-tailwind-purgecss": "0.2.0",
    "vitest": "^1.1.3",
    "vitest-dom": "^0.1.1",
    "vitest-mock-extended": "^1.3.1"
  },
  "type": "module",
  "dependencies": {
    "@flaticon/flaticon-uicons": "^3.3.1",
    "@floating-ui/dom": "1.5.3",
    "@internationalized/date": "^3.5.1",
    "@lucia-auth/adapter-prisma": "^3.0.2",
    "@prisma/client": "^5.6.0",
    "@types/node-steam-openid": "^1.0.3",
    "@vercel/analytics": "^1.1.1",
    "bits-ui": "^0.11.8",
    "chart.js": "^4.4.1",
    "chartjs-adapter-dayjs-4": "^1.0.4",
    "clsx": "^2.0.0",
    "dayjs": "^1.11.10",
    "dota2-css-hero-sprites": "^2.6.1",
    "lucia": "^2.7.4",
    "node-steam-openid": "^1.2.1",
    "radix-icons-svelte": "^1.2.1",
    "svelte-chartjs": "^3.1.2",
    "tailwind-merge": "^2.2.0",
    "tailwind-variants": "^0.1.19"
  },
  "msw": {
    "workerDirectory": "static"
  },
  "overrides": {
    "svelte-chartjs": {
      "svelte": ">=4.x"
    }
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
