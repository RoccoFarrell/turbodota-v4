{
  "name": "turbodota-v4",
  "version": "4.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite dev",
    "dev:prod": "cross-env NODE_ENV=production vite dev",
    "dev:msw-server": "concurrently \"npx ts-node-esm src/mocks/server.ts\" \"npm run dev\"",
    "build": "npx prisma generate && vite build",
    "preview": "cross-env NODE_ENV=production vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "test:e2e": "npx playwright test",
    "coverage": "npx vitest run --coverage",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "prisma": "cross-env NODE_ENV=development npx prisma generate && npx prisma db push",
    "prisma:prod": "cross-env NODE_ENV=production npx prisma generate && npx prisma db push"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@skeletonlabs/skeleton": "2.5.1",
    "@skeletonlabs/tw-plugin": "0.2.4",
    "@sveltejs/adapter-vercel": "^4.0.3",
    "@sveltejs/kit": "^2.0.6",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "0.5.10",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "^4.0.5",
    "@types/node": "^24.10.0",
    "@vitest/coverage-v8": "^4.0.7",
    "@vitest/ui": "^4.0.7",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "jsdom": "^27.1.0",
    "mdsvex": "^0.12.6",
    "node-fetch": "^3.3.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prisma": "^5.22.0",
    "supabase": "^1.115.4",
    "svelte": "^5.43.3",
    "svelte-check": "^4.3.3",
    "svelte-confetti": "^2.3.2",
    "svelte-headless-table": "^0.18.3",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^3.1.1",
    "tailwindcss": "3.3.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.4.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^5.0.0",
    "vite-plugin-tailwind-purgecss": "0.2.0",
    "vitest": "^4.0.7",
    "vitest-dom": "^0.1.1",
    "vitest-mock-extended": "^3.1.0"
  },
  "type": "module",
  "dependencies": {
    "@flaticon/flaticon-uicons": "^3.3.1",
    "@floating-ui/dom": "^1.7.4",
    "@internationalized/date": "^3.10.0",
    "@lucia-auth/adapter-prisma": "^3.0.2",
    "@prisma/client": "^5.22.0",
    "@types/node-steam-openid": "^1.0.3",
    "@vercel/analytics": "^1.5.0",
    "bits-ui": "^0.11.8",
    "chart.js": "^4.5.1",
    "chartjs-adapter-dayjs-4": "^1.0.4",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.19",
    "dota2-css-hero-sprites": "^2.6.1",
    "lucia": "^2.7.7",
    "node-steam-openid": "^1.2.1",
    "radix-icons-svelte": "^1.2.1",
    "svelte-chartjs": "^3.1.2"
  },
  "msw": {
    "workerDirectory": "static"
  },
  "overrides": {
    "svelte-chartjs": {
      "svelte": ">=4.x"
    }
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
